{
  "name": "swingvista",
  "version": "0.1.0",
  "description": "AI-powered golf swing analysis platform with real-time pose detection and feedback",
  "private": true,
  "author": "SwingVista Team",
  "license": "MIT",
  "keywords": [
    "golf",
    "swing-analysis",
    "computer-vision",
    "pose-detection",
    "nextjs",
    "typescript"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:fast": "node scripts/dev-fast.js",
    "dev:minimal": "NODE_OPTIONS='--max-old-space-size=4096' NEXT_CONFIG_FILE=next.config.dev.js node scripts/dev-minimal.js",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out dist",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "@mediapipe/pose": "^0.5.1675469404",
    "@supabase/supabase-js": "^2.38.4",
    "autoprefixer": "^10.4.21",
    "chart.js": "^4.4.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^2.30.0",
    "dotenv": "^17.2.2",
    "next": "14.0.4",
    "postcss": "^8.4.32",
    "react": "^18",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18",
    "tailwindcss": "^3.3.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2.1.4",
    "@next/bundle-analyzer": "^15.5.3",
    "@types/jest": "^29.5.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/ui": "^1.0.4",
    "critters": "^0.0.23",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "cssnano": "^7.1.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "jest": "^29.7.0",
    "jsdom": "^23.0.1",
    "postcss-preset-env": "^10.3.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5",
    "vitest": "^1.0.4"
  }
}
